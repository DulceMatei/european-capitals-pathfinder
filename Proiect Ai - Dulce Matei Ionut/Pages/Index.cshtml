        <div class="col-md-6">
            <div class="card">
                <div class="card-body">
                    <form id="pathFindingForm">
                        <div class="mb-3">
                            <label for="startCountry" class="form-label">Start Country:</label>
                            <select id="startCountry" class="form-select">
                                @foreach (var country in Model.Countries)
                                {
                                    <option value="@country">@country</option>
                                }
                            </select>
                        </div>

                        <div class="mb-3">
                            <label for="endCountry" class="form-label">End Country:</label>
                            <select id="endCountry" class="form-select">
                                @foreach (var country in Model.Countries)
                                {
                                    <option value="@country">@country</option>
                                }
                            </select>
                        </div>

                        <div class="mb-3">
                            <label for="algorithm" class="form-label">Path Finding Algorithm:</label>
                            <select id="algorithm" class="form-select">
                                <option value="bfs">Breadth-First Search (BFS)</option>
                                <option value="astar">A* Search</option>
                            </select>
                        </div>

                        <button type="button" onclick="findPath()" class="btn btn-primary w-100">Find Path</button>
                        <button onclick="resetSelection()" class="btn btn-primary w-100 mt-2">Reset Selection</button>
                        <div id="errorMessage" class="error-message d-flex justify-content-center"></div>


                    </form>
                </div>
            </div>

            <div id="pathResult" class="mt-3">
                <div class="card">
                    <div class="card-body">
                        <h5>Path Details:</h5>
                        <p id="pathText" class="text-muted">Path will be displayed here</p>
                        <p id="totalDistance" class="text-muted">Total distance will be shown here</p>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="mt-4 d-none">
        <h3>Selected Countries:</h3>
        <div id="selectedCountriesDisplay">
            Țări selectate:
            <span id="countriesList">
                @string.Join(", ", Model.SelectedCountries)
            </span>
        </div>
        <button onclick="resetSelection()" class="btn btn-secondary mt-2">Reset Selection</button>
    </div>
</div>
<div id="algorithmMetrics" class="mt-3">
    <div class="card">
        <div class="card-body">
            <h5>Algorithm Performance:</h5>
            <p id="executionTime" class="text-muted">Execution time will be shown here</p>
            <p id="nodesVisited" class="text-muted">Number of visited nodes will be shown here</p>
        </div>
    </div>
</div>
<form asp-page="Index" method="post" id="csrfForm">
    @Html.AntiForgeryToken()
</form>

<script>
        function selectCountry(countryId) {
        // Verificam daca tara este deja selectata
        if (selectedCountries.includes(countryId)) {
            return;
        }
        // Verificam limita de 2 tari
        if (selectedCountries.length >= 2) {
            document.getElementById('errorMessage').textContent = "You cannot select more than 2 countries!";
            return;
        }
        // Selectam token-ul CSRF
        const csrfToken = document.querySelector('input[name="__RequestVerificationToken"]');
        if (!csrfToken) {
            console.error('CSRF token not found');
            return;
        }
        // Trimitem POST request pentru a salva selectia
        fetch('/Index?handler=SelectCountry', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/x-www-form-urlencoded',
                'RequestVerificationToken': csrfToken.value
            },
            body: `countryId=${countryId}`
        })
        .then(response => {
            if (!response.ok) {
                throw new Error('Network response was not ok');
            }
            return response.json();
        })
        .then(data => {
            // Actualizam interfata fara refresh
            if (data.success) {
                // Adăugam tara in lista de tari selectate
                selectedCountries.push(countryId);
                // Actualizam afisarea tarilor
                document.getElementById('countriesList').textContent =
                    selectedCountries.join(", ");

                if (selectedCountries.length === 1) {
                    document.getElementById('startCountry').value = countryId;
                } else if (selectedCountries.length === 2) {
                    document.getElementById('endCountry').value = countryId;
                }

                document.getElementById('errorMessage').textContent = "";
                // Coloram tara selectata
                const countryElement = document.getElementById(countryId);
                if (countryElement) {
                    countryElement.classList.add('country-selected');
                }
            }
        })
        .catch(error => {
            console.error('Error:', error);
        });
    }

        function findPath() {
        const startCountry = document.getElementById('startCountry').value;
        const endCountry = document.getElementById('endCountry').value;
        const algorithm = document.getElementById('algorithm').value;
        const csrfToken = document.querySelector('input[name="__RequestVerificationToken"]');

        fetch('/Index?handler=FindPath', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/x-www-form-urlencoded',
                'RequestVerificationToken': csrfToken.value
            },
            body: `startCountry=${startCountry}&endCountry=${endCountry}&algorithm=${algorithm}`
        })
        .then(response => response.json())
        .then(data => {
            document.getElementById('pathText').innerText =
                data.pathWithCapitals ? data.pathWithCapitals.join(' → ') : 'No path found';
            document.getElementById('totalDistance').innerText =
                data.distance ? `Total Distance: ${data.distance} km` : '';

            document.getElementById('executionTime').innerText =
                data.executionTime ? `Execution Time: ${data.executionTime}` : '';
            document.getElementById('nodesVisited').innerText =
                data.nodesVisited ? `Nodes Visited: ${data.nodesVisited}` : '';

            updateMapPath(data.path);
        })
        .catch(error => {
            console.error('Error:', error);
        });
    }

    function updateMapPath(path) {
        if (!path) return;

        document.querySelectorAll('.map-path').forEach(el => {
            el.classList.remove('map-path');
        });

        // Coloreaza tarile din traseu
        path.forEach(country => {
            const countryElement = document.getElementById(country);
            if (countryElement) {
                countryElement.classList.add('map-path');
            }
        });
    }

        function resetSelection() {
        const csrfToken = document.querySelector('input[name="__RequestVerificationToken"]');
        fetch('/Index?handler=ResetSelection', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/x-www-form-urlencoded',
                'RequestVerificationToken': csrfToken.value
            }
        })
        .then(response => response.json())
        .then(data => {
            if (data.success) {
                // Resetam lista de tari
                selectedCountries = [];
                // Actualizam afisarea
                document.getElementById('countriesList').textContent = "";
                // Resetam dropdown-urile
                if (document.getElementById('startCountry').options.length > 0) {
                    document.getElementById('startCountry').selectedIndex = 0;
                }
                if (document.getElementById('endCountry').options.length > 0) {
                    document.getElementById('endCountry').selectedIndex = 0;
                }
                document.querySelectorAll('.country-selected, .map-path').forEach(el => {
                    el.classList.remove('country-selected', 'map-path');
                });
            }
        });
    }

    // Tarile deja selectate la incarcarea paginii
    let selectedCountries = [@Html.Raw(string.Join(",", Model.SelectedCountries.Select(c => $"'{c}'")))]
</script>


